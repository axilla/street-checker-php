<?php

namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * RaportImage
 *
 * @ORM\Table(name="raport_image")
 * @ORM\Entity(repositoryClass="AppBundle\Repository\RaportImageRepository")
 */
class RaportImage
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @var string
     *
     * @ORM\Column(name="cdn", type="string", length=255, nullable=true)
     */
    private $cdn;

    /**
     * @var string
     *
     * @ORM\Column(name="folder", type="string", length=255, nullable=true)
     */
    private $folder;

    /**
     * @var string
     *
     * @ORM\Column(name="extension", type="string", length=10, nullable=true)
     */
    private $extension;

    /**
     * @var string
     *
     * @ORM\Column(name="name", type="string", length=1024, nullable=true)
     */
    private $name;

    /**
     * @var string
     *
     * @ORM\Column(name="full_url", type="string", length=1024, nullable=true)
     */
    private $fullUrl;

    /**
     * @var string
     *
     * @ORM\Column(name="cdn_full_url", type="string", length=1024, nullable=true)
     */
    private $cdnFullUrl;

    /**
     * @var string
     *
     * @ORM\Column(name="original_full_url", type="string", length=1024, nullable=true)
     */
    private $originalFullUrl;


    /******************** */
    /**** Relations ***** */
    /******************** */

    /**
     * @ORM\ManyToOne(targetEntity="Raport", inversedBy="raportImages")
     * @ORM\JoinColumn(name="raport", referencedColumnName="id")
     * */
    private $raport;

    /****************************/
    /****** END Relations *******/
    /****************************/

    /***********************/
    /*** Some Basics ****/
    /***********************/

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="creation_time", type="datetime")
     */
    private $creationTime;

    /**
     * @var boolean
     *
     * @ORM\Column(name="active", type="boolean")
     */
    private $active;

    /*** END Basics ****/

    public function __construct() {
        $this->setCreationTime(new \DateTime());
        $this->active = 1;
    }

    public function __toString() {
        return $this->name;
    }


    /**
     * Get id
     *
     * @return int
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set cdn
     *
     * @param string $cdn
     *
     * @return RaportImage
     */
    public function setCdn($cdn)
    {
        $this->cdn = $cdn;

        return $this;
    }

    /**
     * Get cdn
     *
     * @return string
     */
    public function getCdn()
    {
        return $this->cdn;
    }

    /**
     * Set folder
     *
     * @param string $folder
     *
     * @return RaportImage
     */
    public function setFolder($folder)
    {
        $this->folder = $folder;

        return $this;
    }

    /**
     * Get folder
     *
     * @return string
     */
    public function getFolder()
    {
        return $this->folder;
    }

    /**
     * Set extension
     *
     * @param string $extension
     *
     * @return RaportImage
     */
    public function setExtension($extension)
    {
        $this->extension = $extension;

        return $this;
    }

    /**
     * Get extension
     *
     * @return string
     */
    public function getExtension()
    {
        return $this->extension;
    }

    /**
     * Set name
     *
     * @param string $name
     *
     * @return RaportImage
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name
     *
     * @return string
     */
    public function getName()
    {
        return $this->name;
    }

    /**
     * Set fullUrl
     *
     * @param string $fullUrl
     *
     * @return RaportImage
     */
    public function setFullUrl($fullUrl)
    {
        $this->fullUrl = $fullUrl;

        return $this;
    }

    /**
     * Get fullUrl
     *
     * @return string
     */
    public function getFullUrl()
    {
        return $this->fullUrl;
    }

    /**
     * Set cdnFullUrl
     *
     * @param string $cdnFullUrl
     *
     * @return RaportImage
     */
    public function setCdnFullUrl($cdnFullUrl)
    {
        $this->cdnFullUrl = $cdnFullUrl;

        return $this;
    }

    /**
     * Get cdnFullUrl
     *
     * @return string
     */
    public function getCdnFullUrl()
    {
        return $this->cdnFullUrl;
    }

    /**
     * Set originalFullUrl
     *
     * @param string $originalFullUrl
     *
     * @return RaportImage
     */
    public function setOriginalFullUrl($originalFullUrl)
    {
        $this->originalFullUrl = $originalFullUrl;

        return $this;
    }

    /**
     * Get originalFullUrl
     *
     * @return string
     */
    public function getOriginalFullUrl()
    {
        return $this->originalFullUrl;
    }

    /**
     * Set creationTime
     *
     * @param \DateTime $creationTime
     *
     * @return RaportImage
     */
    public function setCreationTime($creationTime)
    {
        $this->creationTime = $creationTime;

        return $this;
    }

    /**
     * Get creationTime
     *
     * @return \DateTime
     */
    public function getCreationTime()
    {
        return $this->creationTime;
    }

    /**
     * Set active
     *
     * @param boolean $active
     *
     * @return RaportImage
     */
    public function setActive($active)
    {
        $this->active = $active;

        return $this;
    }

    /**
     * Get active
     *
     * @return boolean
     */
    public function getActive()
    {
        return $this->active;
    }

    /**
     * Set raport
     *
     * @param \AppBundle\Entity\Raport $raport
     *
     * @return RaportImage
     */
    public function setRaport(\AppBundle\Entity\Raport $raport = null)
    {
        $this->raport = $raport;

        return $this;
    }

    /**
     * Get raport
     *
     * @return \AppBundle\Entity\Raport
     */
    public function getRaport()
    {
        return $this->raport;
    }
}
